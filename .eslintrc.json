{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [
        // "google",
        "eslint:recommended",
        "react-app"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "off",
        "jsx-a11y/anchor-is-valid": "off"
    }
}